<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Category</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>
        
            @foreach (var p in Model)
            {
                <tr>
                    <th scope="row">@p.pid</th>
                    <td>@p.pname</td>
                    <td>@p.pcat</td>
                    <td>@p.price</td>
                </tr>
            }
       
        
    </tbody>
</table>