@{
}


@model Combine


Emp Table

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">salary</th>
           
        </tr>
    </thead>
    <tbody>

        @foreach (var obj1 in Model.emps)
        {
            <tr>
                <th scope="row">@obj1.id</th>
                <td>@obj1.name</td>
                <td>@obj1.salary</td>
                
            </tr>
        }


    </tbody>
</table>

<br /> <br />

PRoduct Details

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Category</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var obj2 in Model.prd)
        {
            <tr>
                <th scope="row">@obj2.pid</th>
                <td>@obj2.pname</td>
                <td>@obj2.pcat</td>
                <td>@obj2.price</td>
            </tr>
        }


    </tbody>
</table>